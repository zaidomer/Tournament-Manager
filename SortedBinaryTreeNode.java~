class SortedBinaryTreeNode<E>{
    private T item;
    private Node<T> left;
    private Node<T> right;

    public Node(T item) {
        this.item = item;
        this.right = null;
        this.left = null;
    }

    // public Node(T item, Node<T> next) {
    //     this.item=item;
    //     this.next=next;
    // }

    public Node<T> getLeft(){
        return this.left;
    }

    public Node<T> getRight(){
        return this.right;
    }

    public Node<T> getItem(){
        return this.item;
    }

    public void setLeft(Node<T> leftItem){
        this.left = leftItem;
    }

    public void setRight(Node<T> rightItem){
        this.right = rightItem;
    }

    public Node<T> setItem(Node<T> item){
        this.item = item;
    }

    boolean isLeaf(){
        if(left == null && right == null){
            return true;
        }else{
            return false;
        }
    }
}